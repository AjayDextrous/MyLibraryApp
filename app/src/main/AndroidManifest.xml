<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ajay.library_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.ajay.library_app.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ajay.library_app.NewBookScreenActivity" />
        <activity android:name="com.example.ajay.library_app.AdminLibraryActivity" />
        <activity android:name="com.example.ajay.library_app.UserViewLibraryActivity" />
        <activity android:name="com.example.ajay.library_app.UserLibraryActivity" />
        <activity android:name="com.example.ajay.library_app.swipeBorrowActivity" />
        <activity android:name="com.example.ajay.library_app.swipeEditActivity" />
        <activity android:name="com.example.ajay.library_app.TestActivity"/>
        <activity android:name="com.example.ajay.library_app.swipeReturnActivity"></activity>
    </application>

</manifest>